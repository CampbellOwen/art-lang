{
  "name": "art-lang",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon -w src/server -x tsx src/server/main.ts",
    "start": "NODE_ENV=production tsx src/server/main.ts",
    "build": "vite build",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules\" jest",
    "test:watch": "NODE_OPTIONS=\"--experimental-vm-modules\" jest --watch",
    "test:debug": "NODE_OPTIONS=\"--experimental-vm-modules\" node --inspect-brk node_modules/.bin/jest --runInBand --no-cache",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "yarn run lint && yarn run format:check",
    "fix": "yarn run lint:fix && yarn run format"
  },
  "dependencies": {
    "express": "^5.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "vite-express": "*"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@jest/globals": "^30.0.4",
    "@types/express": "^5.0.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.15.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.11.0",
    "jest": "^30.0.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.3.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.8.0",
    "vite": "^6.3.3"
  }
}
